<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | SRAVANTHI HIS</title>
    <link href="~/Content/Site.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/his_customstyles.css" rel="stylesheet" />
    <style type="text/css">
        .his-site-header {
            padding: 12.5px !important;
            background-color: #fff !important;
            border-bottom: solid 1px #e3e1e1 !important;
        }

        .page-container.sidebar-collapsed .site-header {
            padding: 18px !important;
        }

        .his-icon-side-menu {
            color: #212529 !important
        }
    </style>
</head>
<body>
    <!-- Page container -->
    <div class="page-container sidebar-collapsed">
        <!-- Page Sidebar -->
        <div class="page-sidebar collapsed">

            <!-- Site header  -->
            <header class="site-header his-site-header">
                <div class="site-logo"><a href="index.html"><img src="~/light/images/hms_logo_menu.png" alt="Mouldifi" title="Mouldifi"></a></div>
                <div class="sidebar-collapse hidden-xs"><a data-target="#side-nav" class="sidebar-collapse-icon" href="#"><i class="fa fa-bars his-icon-side-menu"></i></a></div>
                <div class="sidebar-mobile-menu visible-xs"><a data-target="#side-nav" data-toggle="collapse" class="mobile-menu-icon" href="#"><i class="fa fa-bars his-icon-side-menu"></i></a></div>
            </header>
            <!-- /site header -->
            <!-- Main navigation -->
            <ul id="side-nav" class="main-menu navbar-collapse collapsed">
                <li class="has-sub">
                    <a href="/home/index"><i class="icon-home"></i><span class="title">Home</span></a>
                    <ul class="nav"></ul>
                </li>
                <li class="has-sub">
                    <a href="@Url.Action("Index", "InPatient")"><i class="fa fa-wheelchair" style="padding-left:5px"></i><span class="title">Patients</span></a>
                    <ul class="nav">
                        <li><a href="@Url.Action("Index", "InPatient")"><span class="title">I/P Profile</span></a></li>
                        <li><a href="@Url.Action("Index", "OutPatient")"><span class="title">O/P Profile</span></a></li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a href="@Url.Action("Index", "Rooms")"><i class="fa fa-bed" style="padding-left:5px"></i><span class="title">Rooms</span></a>
                    <ul class="nav">
                        <li><a href="@Url.Action("Index", "Rooms")"><span class="title">Room Allocation</span></a></li>
                        <li><a href="@Url.Action("Index", "Beds")"><span class="title">Bed Management</span></a></li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a href="@Url.Action("Index", "MedInventory")"><i class="fa fa-medkit" style="padding-left:5px"></i><span class="title">Pharmacy</span></a>
                    <ul class="nav">
                        <li><a href="@Url.Action("Index", "PurchaseOrder")"><span class="title">Purchase Orders</span></a></li>
                        <li><a href="#/"><span class="title">Patient Prescription</span></a></li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a href="index.html"><i class="icon-chart-bar"></i><span class="title">Reports</span></a>
                    <ul class="nav">
                        <li class="active"><a href="index.html"><span class="title">Report 1</span></a></li>
                        <li><a href="ecommerce-dashboard.html"><span class="title">Report 2</span></a></li>
                        <li><a href="news-dashboard.html"><span class="title">Report 3</span></a></li>
                    </ul>
                </li>
                <li class="has-sub active">
                    <a href="@Url.Action("Index", "User")"><i class="icon-lock"></i><span class="title">Admin</span></a>
                    <ul class="nav">
                        <li class="active"><a href="@Url.Action("Index", "User")"><span class="title">Users</span></a></li>
                        <li><a href="@Url.Action("Index", "BloodGroup")"><span class="title">Blood Groups</span></a></li>
                        <li><a href="@Url.Action("Index", "UserType")"><span class="title">User Types</span></a></li>
                        <li><a href="@Url.Action("Index", "Specialization")"><span class="title">Specialization</span></a></li>
                        <li><a href="@Url.Action("Index", "Intakes")"><span class="title">Intake Frequency</span></a></li>
                        <li><a href="@Url.Action("Index", "Brand")"><span class="title">Brands</span></a></li>
                        <li><a href="@Url.Action("Index", "BrandCategory")"><span class="title">Brand Categories</span></a></li>
                        <li><a href="@Url.Action("Index", "ConsultType")"><span class="title">Consultations</span></a></li>
                        <li><a href="@Url.Action("Index", "ConsultationFee")"><span class="title">Consultation Fee</span></a></li>
                        <li><a href="@Url.Action("Index", "MedicineMaster")"><span class="title">Medicines</span></a></li>
                    </ul>
                </li>
            </ul>
            <!-- /main navigation -->
        </div>
        <!-- /page sidebar -->
        <!-- Main container -->
        <div class="main-container">
            <!-- Main header -->
            <div class="main-header row">
                <div class="col-sm-6 col-xs-7">
                    <!-- User info -->
                    <ul class="user-info pull-left">
                        <li class="profile-info dropdown">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#" aria-expanded="false">Girish Rebba<span class="caret"></span></a>
                            <!-- User action menu -->
                            <ul class="dropdown-menu">
                                <li><a href="#/"><i class="icon-user"></i>My profile</a></li>
                                <li><a href="#/"><i class="icon-logout"></i>Logout</a></li>
                            </ul>
                            <!-- /user action menu -->
                        </li>
                    </ul>
                    <!-- /user info -->
                </div>
                <div class="col-sm-6 col-xs-5">
                    <div class="pull-right">
                        <!-- User alerts -->
                        <ul class="user-info pull-left">
                            <!-- Notifications -->
                            <li class="notifications dropdown">
                            </li>
                            <!-- /notifications -->
                            <!-- Messages -->
                            <li class="notifications dropdown">
                                <a data-close-others="true" data-hover="dropdown" data-toggle="dropdown" class="dropdown-toggle" href="#"><i class="icon-mail"></i><span class="badge badge-secondary">12</span></a>
                                <ul class="dropdown-menu pull-right">
                                    <li class="first">
                                        <div class="dropdown-content-header"><i class="fa fa-pencil-square-o pull-right"></i> Messages</div>
                                    </li>

                                    <li class="external-last"> <a class="danger" href="#">All Messages</a> </li>
                                </ul>
                            </li>
                            <!-- /messages -->
                        </ul>
                        <!-- /user alerts -->
                    </div>
                </div>
            </div>
            <!-- /main header -->
            <!-- Main content -->
            <div class="main-content">
                @RenderBody()
            </div>
            <!-- /main content -->
        </div>
        <!-- /main container -->
    </div>
    <!-- /page container -->
    <!--Load JQuery-->
    <!-- Footer -->
    <footer class="footer-main">
        &copy; 2017 <strong>Hospital Information System</strong><a target="_blank" href="#/"></a>
    </footer>
    <!-- /footer -->
    <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/gridmvc.min.js"></script>
    <script src="~/light/js/plugins/metismenu/jquery.metisMenu.js"></script>
    <script src="~/light/js/plugins/blockui-master/jquery-ui.js"></script>
    <script src="~/light/js/plugins/blockui-master/jquery.blockUI.js"></script>
    <script src="~/light/js/functions.js"></script>
    <script src="~/light/js/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/light/js/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <script src="~/light/js/plugins/datatables/extensions/Buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/light/js/plugins/datatables/jszip.min.js"></script>
    <script src="~/light/js/plugins/datatables/pdfmake.min.js"></script>
    <script src="~/light/js/plugins/datatables/vfs_fonts.js"></script>
    <script src="~/light/js/plugins/datatables/extensions/Buttons/js/buttons.html5.js"></script>
    <script src="~/light/js/plugins/datatables/extensions/Buttons/js/buttons.colVis.js"></script>
    <link href="~/light/js/plugins/datatables/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/light/js/plugins/datatables/extensions/Buttons/css/buttons.dataTables.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script src="~/Scripts/notify.min.js"></script>
    <link href="~/light/css/plugins/datepicker/bootstrap-datepicker.css" rel="stylesheet" />
    <script src="~/Scripts/moment.js"></script>
    <script src="~/light/js/plugins/datepicker/bootstrap-datepicker.js"></script>
    <script type='text/javascript'>
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>

