@model HIS.RoomType

@{
    Layout = null;
}

@using (Html.BeginForm("AddModify", "RoomType", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.RoomTypeID)
    <div class="form-group">
        @Html.LabelFor(model => model.RoomType1, "Room Type",new { @class = "control-label" })
      
            @Html.EditorFor(model => model.RoomType1, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.RoomType1)
       

    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn" />
    </div>
}
