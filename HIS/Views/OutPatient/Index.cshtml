        function SubmitPrescriptionForm(form) {
            $('body').loadingModal({ text: 'Processing...' });
            var ppItems = [];
            var enmrNo = $('#ENMRNO').val();
            var visitID = $('#VisitID').val();
            if ($(form).find('table#ppTable tr').length >= 3) {
                $('#MedicineWithDose').rules('remove', 'required');
                $('#Quantity').rules('remove', 'required');
                $('#IntakeFrequencyID').rules('remove', 'required');
                $('#ppTable > tbody tr').not(':first').each(function (index, elem) {
                    var tbl = {};
                    tbl.ENMRNO = enmrNo;
                    tbl.VisitID = visitID;
                    tbl.MedicineID = $($(this).find('td:eq(0)')).html();
                    tbl.MedicineWithDose = $($(this).find('td:eq(1)')).html();
                    tbl.Quantity = $($(this).find('td:eq(2)')).html();
                    tbl.IntakeFrequencyID = $($(this).find('td:eq(3)')).html();
                    tbl.Comments = $($(this).find('td:eq(5)')).html();
                    tbl.TestIds = $('#TestIds').val();
                    tbl.ScanIds = $('#ScanIds').val();
                    tbl.KitIds = $('#KitIds').val();
                    tbl.HasPrescription = true;
                    ppItems.push(tbl);
                });
            }
            else {
                $.validator.setDefaults({
                    ignore: []
                });
                var tbl = {};
                tbl.ENMRNO = enmrNo;
                tbl.VisitID = visitID;
                tbl.TestIds = $('#TestIds').val();
                tbl.ScanIds = $('#ScanIds').val();
                tbl.KitIds = $('#KitIds').val();
                tbl.HasPrescription = false;
                ppItems.push(tbl);
            }

            $(form).find('#headerDiv').append("</br><table><tr><td>ENMRNO: " + $('#ENMRNO').val() + "</td></tr><tr><td>Doctor: " + $('#DoctorName').val() + "</td></tr><tr><td>Visit & Date: " + $('#VisitName').val() + "</td></tr></table>");
            $(form).find('#headerDiv').append($(form).find('#ppTable'));
            $('#fstRow').closest('tr').remove();
            $('#thd').find('th:last').remove();
            $("#tbd tr").each(function () {
                $(this).find('td:last').remove();
            });
            var option_all = '';
            if ($('#TestIds').val() != null) {
                option_all = $("#TestIds option:selected").map(function () {
                    return $(this).text();
                }).get().join(',');
            }

            if ($('#KitIds').val() != null) {
                option_all = $("#KitIds option:selected").map(function () {
                    return $(this).text();
                }).get().join(',');
            }
            var str_array = option_all.split(',');
            var tbodyTR = '';
            var tbodtTD = '';
            for (var i = 0; i < str_array.length; i++) {
                tbodtTD += '<tr><td>'+str_array[i].replace(/^\s*/, "").replace(/\s*$/, "")+'</td></tr>'
            }
            tbodyTR +=  tbodtTD;
            $(form).find('#headerDiv').append("</br><table class='table table-striped table-bordered'><thead><tr><td>Test Name</td></tr></thead><tbody>" + tbodyTR + "</tbody></table>");


            var option_all1 = $("#ScanIds option:selected").map(function () {
                return $(this).text();
            }).get().join(',');

            var str_array1 = option_all1.split(',');
            var tbodyTR1 = '';
            var tbodtTD1 = '';
            for (var i = 0; i < str_array1.length; i++) {
                tbodtTD1 += '<tr><td>' + str_array1[i].replace(/^\s*/, "").replace(/\s*$/, "") + '</td></tr>'
            }
            tbodyTR1 += tbodtTD1;
            $(form).find('#headerDiv').append("</br><table class='table table-striped table-bordered'><thead><tr><td>Scan Name</td></tr></thead><tbody>" + tbodyTR1 + "</tbody></table>");




            $.ajax({
                type: "POST",
                contentType: "application/json",
                url: form.action,
                data: JSON.stringify(ppItems),
                success: function (data) {
                    if (data.success) {
                        $('#headerDiv').printThis({
                            header: '<img src="http://localhost:50415/light/images/hms-logo_login.png" style="width:420px;" alt="Mouldifi" title="Sravanthi Hospitals">',
                            footer: false,
                            pageTitle: ""
                        });
                        newPP.dialog('close');
                        dataTable.ajax.reload();
                        $.notify(data.message, {
                            globalPosition: "top center",
                            className: "success"
                        })
                    }
                    $('body').loadingModal('destroy');
                }
            });
            return false;
        }

        function SubmitDeliverPrescriptionForm(form) {
            $('body').loadingModal({ text: 'Processing...' });
            var prescriptions = [];
            var enmrNo = $('#ENMRNO').val();
            var visitID = $('#VisitID').val();
            var discount = $('#Discount').val();
            var amountToBePaid = $('#paidAmount').html();
            var totalAmount = $('#grandTotal').html();
            var PmID = $('#PMID').val();
            $(form).find('#ppTable > tbody tr').each(function (index, elem) {
                var tbl = {};
                tbl.ENMRNO = enmrNo;
                tbl.VisitID = visitID;
                tbl.MedicineID = $($(this).find('td:eq(1)')).html();
                tbl.DeliverQty = $($(this).find('td:eq(4)')).find('.deliverQty').val();
                tbl.RequestQty = $($(this).find('td:eq(5)')).find('.reqQty').val();
                tbl.BatchNo = $($(this).find('td:eq(8)')).find('.batchNo').val();
                tbl.LotNo = $($(this).find('td:eq(9)')).find('.lotNo').val();
                tbl.Discount = discount;
                tbl.PaidAmount = amountToBePaid;
                tbl.TotalAmount = totalAmount;
                tbl.PMID = PmID;
                tbl.MedicineWithDose = 'demo';
                prescriptions.push(tbl);
            });
            $(form).find('#headerDivdp').append("</br><table><tr><td>ENMRNO: " + $('#ENMRNO').val() + "</td></tr><tr><td>Doctor: " + $('#DoctorName').val() + "</td></tr><tr><td>Visit: " + $('#VisitName').val() + "</td></tr><tr><td colspan='2'>Suggested Date: " + $('#DateDisplay').val() + "</td></tr></table>");
            $(form).find('#headerDivdp').append($(form).find('#ppTable'));

            $.ajax({
                type: "POST",
                contentType: "application/json",
                url: form.action,
                data: JSON.stringify(prescriptions),
                success: function (data) {
                    if (data.success) {
                        $('#headerDivdp').printThis({
                            header: '<img src="http://localhost:50415/light/images/hms-logo_login.png" style="width:420px;" alt="Mouldifi" title="Sravanthi Hospitals">',
                            footer: false,
                            pageTitle: ""
                        });
                        deliverPopUp.dialog('close');
                        dataTable.ajax.reload();
                        $.notify(data.message, {
                            globalPosition: "top center",
                            className: "success"
                        })
                    }
                    $('body').loadingModal('destroy');
                }
            });
            return false;
        }

        function SubmitMDRForm(form) {
            $('body').loadingModal({ text: 'Processing...' });
            var mdrRequest = [];
            $.validator.setDefaults({
                ignore: []
            });
            var enmrNo = $('#ENMRNO').val();
            var doctorID = $('#DoctorID').val();
            var discount = $('#Discount').val();
            var amountPaid = $('#paidAmount').html();
            var totalAmount = $('#grandTotal').html();
            $(form).find('#mdrTable > tbody tr').not(':first').each(function (index, elem) {
                var tbl = {};
                tbl.ENMRNO = enmrNo;
                tbl.DoctorID = doctorID,
                tbl.IsDelivered = true,
                tbl.VisitID = 0;
                tbl.MedicineID = $($(this).find('td:eq(0)')).html();
                tbl.IntakeFrequencyID = $($(this).find('td:eq(3)')).html();
                tbl.DeliverQty = $($(this).find('td:eq(2)')).html();
                tbl.Quantity = $($(this).find('td:eq(2)')).html();
                tbl.BatchNo = $($(this).find('td:eq(5)')).html();
                tbl.LotNo = $($(this).find('td:eq(6)')).html();
                tbl.Discount = discount;
                tbl.PaidAmount = amountPaid;
                tbl.TotalAmount = totalAmount;
                mdrRequest.push(tbl);
            });
            $(form).find('#headerDiv').append("</br><table><tr><td>ENMRNO: " + $('#ENMRNO').val() + "</td></tr><tr><td>Doctor: " + $("#PrescribedByID option:selected").text() + "</td></tr></table>");
            $(form).find('#headerDiv').append($(form).find('#mdrTable'));
            $('#fstRow').closest('tr').remove();
            $('#thd').find('th:last').remove();
            $('#tbd').find('td:last').remove();
            $("#ftr tr").each(function () {
                $(this).find("td").attr('colspan', 3);
            });


            $.ajax({
                type: "POST",
                contentType: "application/json",
                url: form.action,
                data: JSON.stringify(mdrRequest),
                success: function (data) {
