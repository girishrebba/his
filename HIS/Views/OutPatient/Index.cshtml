                Popup = formDiv.dialog({
                    autoOpen: true,
                    resizable: true,
                    modal:true,
                    title: id == 0? 'Create Out Patient' : 'Edit Out Patient',
                    width: 850,
                    top: 147,
                    close: function () {
                        Popup.dialog('destroy').remove();
                    },
                    open: function (event, ui) {
                        $('.ui-dialog-content').css('overflow', 'hidden');
                        $('body').loadingModal('destroy');
                    }
                });
            });
        }

